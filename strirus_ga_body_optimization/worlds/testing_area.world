    <!-- max_real_time_factor = real_time_update_rate * max_step_size -->
    <!-- min_real_time_factor depends only from max_step_size --> 
    <real_time_update_rate>111.2</real_time_update_rate>
    <max_step_size>0.009 </max_step_size>
      <ode>
        <solver>
          <type>quick</type>
          <iters>400</iters>
        </solver>
      </ode>
    </physics>  
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>
        <include>
		<uri>model://strirus_gazebo/maps/Generated_terrain</uri>
      <name>Terrain</name>
      <pose>0 -2.0 0 0 0 0</pose>
    </include>
  </world>
</sdf>
